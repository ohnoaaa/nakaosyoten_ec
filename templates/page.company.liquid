<main>
  <div class="pages_bg_sp">
    <section class="subapge_head fadeInTrigger">
      <div class="head_img">
        <img
          src="https://cdn.shopify.com/s/files/1/0705/7381/8094/files/subpages_head_bg.png"
          alt=""
          width="968"
          height="968"
        >
      </div>
      <h1 class="subapge_jp">会社概要</h1>
      <p class="subpage_txt">COMPANY</p>
      <div class="pagenav">
        <a href="#com_info" class="btn"
          >会社情報<span
            ><img
              src="https://cdn.shopify.com/s/files/1/0705/7381/8094/files/btn_img.png"
              alt=""
              width="43"
              height="43"></span
        ></a>
        <a href="#shop_info" class="btn"
          >店舗情報<span
            ><img
              src="https://cdn.shopify.com/s/files/1/0705/7381/8094/files/btn_img.png"
              alt=""
              width="43"
              height="43"></span
        ></a>
      </div>
    </section>
    <!-- about_head -->
    <div class="wrapper wrapper_terms fadeUpTrigger">
      <section class="company" id="com_info">
        <div class="com_ttl">
          <div class="com_ttl_bg">
            <img src="https://cdn.shopify.com/s/files/1/0705/7381/8094/files/com_bg.png" alt="">
